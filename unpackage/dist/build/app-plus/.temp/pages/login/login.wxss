@import "../../common/vendor.wxss";
.myLogin{background:#fff}.content{width:100%;height:100%}.main{height:600rpx;text-align:center;padding-top:150rpx;background:-webkit-radial-gradient(#f8f8f8,#89a2fe,#819bfc,#7893fa);background:radial-gradient(#f8f8f8,#89a2fe,#819bfc,#7893fa)}.action-row,.main{box-sizing:border-box}.action-row{position:absolute;top:500rpx;left:0;right:0;width:88%;margin:0 auto;padding:50rpx 60rpx;background:#fff;border-radius:10rpx;box-shadow:10rpx 10rpx 5rpx rgba(0,0,0,.1)}.list{box-sizing:border-box}.list image{width:40rpx;height:40rpx;margin-right:40rpx;-webkit-transform:translateY(4rpx);transform:translateY(4rpx)}.list .name,.list .password{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:horizontal;-webkit-box-direction:normal;-webkit-flex-direction:row;flex-direction:row;margin-top:40rpx;border-bottom:1rpx solid #eee}.wjPass{float:right;color:#999;margin:15rpx 0}.login{margin-top:80rpx;width:100%;border-radius:50rpx;background:-webkit-linear-gradient(#819bfc,#89a2fe,#819bfc);background:linear-gradient(#819bfc,#89a2fe,#819bfc)}.register{-webkit-transform:translateY(-80rpx);transform:translateY(-80rpx);width:100%;height:40rpx;box-sizing:border-box;text-align:center;padding:0 20rpx;color:#999}.register .cont{position:relative}.register .cont image{width:100rpx;height:100rpx}.register .cont:after,.register .cont:before{content:"";position:absolute;top:50%;background:#999;width:35%;height:1rpx}.register .cont:before{left:2%}.register .cont:after{right:2%}